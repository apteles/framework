{
    "name": "apteles/framework",
    "type": "library",
    "description": "The Laravel Framework.",
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "spatie/phpunit-watcher": "^1.21",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.3"
    },
    "authors": [
        {
            "name": "Andre Teles",
            "email": "andre.telestp@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/http-foundation": "^5.0",
        "psr/container": "^1.0",
        "php-di/invoker": "^2.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ApTeles\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApTeles\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit tests --testdox",
        "test:watch": "phpunit-watcher watch"
    }
}
